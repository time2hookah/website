{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    
    {
      "type": "node",
      "request": "attach",
      "name": "Attach by Process ID",
      // "processId": "${command:PickProcess}",
      "processId": "11493",
      "port": 9229,
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      
    },
    {
      "type": "node",
      "request": "launch",
      "protocol": "inspector",
      "name": "Launch: debug",
      "program": "${workspaceFolder}/debug.js",
      "stopOnEntry": true,
      "restart": true,
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
    },

    {
      "type": "node",
      "request": "launch",
      "name": "Launch: webpack",
      "program": "${workspaceFolder}/webpack.config.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
    }, 

    {
      "type": "node",
      "request": "launch",
      "name": "Launch: defaultController",
      "program": "${workspaceFolder}/controllers/default.js",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
    },

    {
      "type": "node",
      "request": "launch",
      "name": "Launch: vue-loader",
      "program": "${workspaceFolder}/node_modules/vue-loader/lib/index.js"
    }

  ]
}